<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/context
          http://www.springframework.org/schema/context/spring-context-4.3.xsd
          http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-4.3.xsd"
    default-autowire="byName">

	<context:mbean-export default-domain="ShowCase" registration="replaceExisting" />

	<import resource="DBSource.xml"/>
	<!--<import resource="PostgresqlTest.xml"/>-->
 	<import resource="SqlServerTest.xml"/>
	<!--
		<import resource="MysqlTest.xml"/>
		<import resource="SqlServerTest.xml"/>
		<import resource="OracleTest.xml"/>
	 -->
	
	<bean id="checkAndAppendTestConf" class="com.cloudbeaver.checkAndAppend.CheckAndAppendTestConf">
		<property name="NewVersion" value="false"/>
    	<property name="databases">
    		<list>
			<ref bean="SqlServerTest"/>
    			<!--<ref bean="PostgresqlTest"/>-->
	    		<!--
	    			<ref bean="MysqlTest"/>
	    			<ref bean="SqlServerTest"/>
	    			<ref bean="OracleTest"/>
	    		 -->
        	</list>
		</property>
    </bean>
</beans>
